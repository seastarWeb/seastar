{
    "name": "nenad/yii2-advanced-template",
    "description": "Improved Yii 2 Advanced Application Template By Nenad Zivkovic",
    "keywords": ["yii2", "framework", "advanced", "improved", "application template", "nenad"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "tutorial": "http://www.freetuts.org/tutorial/view?id=6",
        "source": "https://github.com/nenad-zivkovic/yii2-advanced-template"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "nenad/yii2-password-strength": "*",
        "mihaildev/yii2-ckeditor": "*",
        "yiisoft/yii2-imagine": "~2.0.0",
        "kartik-v/yii2-social": "dev-master",
        "kartik-v/yii2-tabs-x": "dev-master",
        "kartik-v/yii2-nav-x": "dev-master",
        "kartik-v/yii2-detail-view": "*",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-widgets": "dev-master",
        "kartik-v/yii2-label-inplace": "*",
        "kop/yii2-scroll-pager":"dev-master",
        "omnilight/yii2-shopping-cart": "*",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "2amigos/yii2-gallery-widget": "~1.0",
        "dwmsw/sagepay": "dev-master",
        "kartik-v/mpdf": "dev-master",
        "yiisoft/yii2-jui": "~2.0.0",
        "evgeniyrru/yii2-slick" : "*",
        "himiklab/yii2-easy-thumbnail-image-helper" : "*",
        "geedmo/yamm3": "dev-master", 
	"kartik-v/yii2-editable": "@dev",
        "linslin/yii2-curl": "*",
        "execut/yii2-widget-bootstraptreeview": "dev-master",
        "imanilchaudhari/yii2-rrssb": "dev-master",
        "drsdre/yii2-wizardwidget": "*",
        "himiklab/yii2-sitemap-module": "*",
	"scotthuangzl/yii2-google-chart": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "config": {
        "vendor-dir": "_protected/vendor",
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "_protected/vendor/npm",
            "bower-asset-library": "_protected/vendor/bower"
        }
    }
}
